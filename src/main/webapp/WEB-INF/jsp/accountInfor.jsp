                                                <input type="email" name="user_email" id="user_email" required="">
                                                <label>Số điện thoại *</label>
                                                <input type="text" name="user_phone_number" id="user_phone_number" pattern="(84|0[3|5|7|8|9])+([0-9]{8})" required="">
